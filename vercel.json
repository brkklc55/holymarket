{
    "redirects": [
        {
            "source": "/((?!.well-known|manifest.json|icon-1024.png|apple-touch-icon.png|favicon.png).*)",
            "has": [
                {
                    "type": "host",
                    "value": "baseappholymarket.xyz"
                }
            ],
            "destination": "https://www.baseappholymarket.xyz/$1",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/farcaster.json",
            "destination": "/.well-known/farcaster.json"
        },
        {
            "source": "/api/og.png",
            "destination": "/api/og"
        },
        {
            "source": "/api/og/:v.png",
            "destination": "/api/og?v=:v"
        }
    ]
}