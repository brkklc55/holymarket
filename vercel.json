{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "baseappholymarket.xyz"
                }
            ],
            "destination": "https://www.baseappholymarket.xyz/$1",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/farcaster.json",
            "destination": "/api/farcaster.json"
        },
        {
            "source": "/.well-known/farcaster.json",
            "destination": "/api/farcaster.json"
        },
        {
            "source": "/manifest.json",
            "destination": "/api/manifest.json"
        },
        {
            "source": "/.well-known/base-app-config.json",
            "destination": "/api/base-app-config.json"
        },
        {
            "source": "/.well-known/onchain-app-config.json",
            "destination": "/api/onchain-app-config.json"
        },
        {
            "source": "/onchain-app-config.json",
            "destination": "/api/onchain-app-config.json"
        },
        {
            "source": "/favicon.png",
            "destination": "/api/icon?s=32"
        },
        {
            "source": "/apple-touch-icon.png",
            "destination": "/api/icon?s=180"
        },
        {
            "source": "/icon-192.png",
            "destination": "/api/icon?s=192"
        },
        {
            "source": "/icon-512.png",
            "destination": "/api/icon?s=512"
        },
        {
            "source": "/icon-1024.png",
            "destination": "/api/icon?s=1024"
        }
    ]
}