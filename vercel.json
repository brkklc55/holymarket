{
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "baseappholymarket.xyz"
                }
            ],
            "destination": "https://www.baseappholymarket.xyz/$1",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/farcaster.json",
            "destination": "/.well-known/farcaster.json"
        },
        {
            "source": "/favicon.png",
            "destination": "/api/icon?s=32"
        },
        {
            "source": "/apple-touch-icon.png",
            "destination": "/api/icon?s=180"
        },
        {
            "source": "/icon-192.png",
            "destination": "/api/icon?s=192"
        },
        {
            "source": "/icon-512.png",
            "destination": "/api/icon?s=512"
        },
        {
            "source": "/icon-1024.png",
            "destination": "/api/icon?s=1024"
        },
        {
            "source": "/api/og.png",
            "destination": "/api/og"
        },
        {
            "source": "/api/og/:v.png",
            "destination": "/api/og?v=:v"
        }
    ]
}